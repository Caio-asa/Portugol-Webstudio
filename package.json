{
  "name": "portugol-webstudio",
  "version": "1.0.0",
  "description": "IDE online para o Portugol (UNIVALI)",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "deploy": "now --public --team $NOW_TEAM -e NODE_ENV=production --token $NOW_TOKEN --docker",
    "alias": "now --public --team $NOW_TEAM --token=$NOW_TOKEN alias",
    "test": "echo 'Procurando erros de sintaxe...' && find . -type f -not -path \"*node_modules*\" -name \"*.js\" -exec node -c {} \\;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgadelha/Portugol-Webstudio.git"
  },
  "keywords": [
    "portugol",
    "univali",
    "webstudio"
  ],
